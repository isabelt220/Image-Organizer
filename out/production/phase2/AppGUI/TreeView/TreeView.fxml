                                    </ContextMenu>
                                 </contextMenu>
                                 <padding>
                                    <Insets top="50.0" />
                                 </padding>
                              </ListView>
                              <HBox fx:id="hBox" layoutX="7.0" layoutY="21.0" prefHeight="27.0" prefWidth="236.0" visible="false">
