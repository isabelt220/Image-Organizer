<?xml version="1.0" encoding="UTF-8"?>

<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.image.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TableColumn?>
<?import javafx.scene.control.TableView?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.Pane?>
<?import javafx.scene.layout.VBox?>

<Pane prefHeight="431.0" prefWidth="520.0" xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="AppGUI.CenterPanel.MiddleWindowController">
   <children>
      <VBox fx:id="displayImage" prefHeight="431.0" prefWidth="519.0">
         <children>
            <Pane prefHeight="78.0" prefWidth="519.0">
               <children>
                  <TextField fx:id="searchTextField" layoutX="287.0" layoutY="14.0" promptText="Enter Single Tag Here" />
                  <Button fx:id="searchButton" layoutX="444.0" layoutY="14.0" mnemonicParsing="false" onAction="#searchTagClicked" text="Search Tag" />
               </children>
            </Pane>
            <VBox alignment="CENTER" prefHeight="260.0" prefWidth="519.0">
               <children>
                  <ImageView fx:id="imageView" fitHeight="224.0" fitWidth="391.0" onMouseClicked="#check" pickOnBounds="true" preserveRatio="true" />
               </children>
            </VBox>
            <TableView fx:id="tableView" prefHeight="200.0" prefWidth="520.0">
               <columns>
                  <TableColumn fx:id="tableColumn1" prefWidth="260.0" text="image" />
                  <TableColumn fx:id="tableColumn2" minWidth="30.0" prefWidth="260.0" text="tag" />
               </columns>
            </TableView>
         </children>
      </VBox>
   </children>
</Pane>
